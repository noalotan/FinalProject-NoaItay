stage('Architecture Apply') {
    steps {
        script {
            setGitHubPullRequestStatus state: 'pending', context: 'Architecture Apply'
            try {
                build job: 'Dev-Architecture-apply'
                setGitHubPullRequestStatus state: 'success', context: 'Architecture Apply'
            } catch (Exception e) {
                setGitHubPullRequestStatus state: 'failure', context: 'Architecture Apply'
                throw e
            }
        }
    }
}
